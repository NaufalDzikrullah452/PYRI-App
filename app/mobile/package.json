{
  "name": "capacitor-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "capacitor": "npx cap copy && npx cap sync",
    "gradlew": "cd android && ./gradlew clean",
    "build:signed": "cd android && ./gradlew assembleRelease",
    "build:debug": "cd android && ./gradlew assembleDebug --warning-mode all",
    "install:debug": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "install:signed": "adb install -r android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@capacitor/android": "^3.2.3",
    "@capacitor/camera": "latest",
    "@capacitor/core": "^3.2.3",
    "@capacitor/splash-screen": "latest",
    "@capacitor/app": "^1.0.2",
    "@capacitor/browser": "^1.0.2",
    "@capacitor/device": "^1.0.2",
    "@capacitor/geolocation": "^1.0.2",
    "@capacitor/local-notifications": "^1.0.5",
    "@capacitor/push-notifications": "^1.0.1",
    "@capacitor/storage": "^1.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.2.3"
  },
  "author": "",
  "license": "ISC"
}
